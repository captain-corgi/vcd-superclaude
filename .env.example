# Application Configuration
APP_NAME=multi-agent-system
APP_VERSION=1.0.0
APP_DEBUG=true
APP_PORT=8080
APP_TIMEOUT=30s

# Database Configuration
DATABASE_URL=postgres://postgres:password@localhost:5432/multiagent?sslmode=disable
REDIS_URL=redis://localhost:6379

# API Credentials
JIRA_CLIENT_ID=your-jira-client-id
JIRA_CLIENT_SECRET=your-jira-client-secret
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_USERNAME=your-jira-username
JIRA_TOKEN=your-jira-api-token

SLACK_CLIENT_ID=your-slack-client-id
SLACK_CLIENT_SECRET=your-slack-client-secret
SLACK_SIGNING_SECRET=your-slack-signing-secret

GITHUB_TOKEN=your-github-personal-access-token

# Agent Configuration
PLANNER_MAX_REQUESTS=10
PLANNER_TIMEOUT=30s

EXECUTOR_RATE_LIMIT=100
EXECUTOR_RETRY_COUNT=3

EVALUATOR_CACHE_TTL=1h
EVALUATOR_MAX_REPORT_SIZE=10485760

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE=

# Development Configuration
DEV_MODE=true
DATABASE_MIGRATIONS=true

# Security Configuration
JWT_SECRET=your-jwt-secret-key
CORS_ENABLED=true
CORS_ORIGINS=*

# Monitoring Configuration
METRICS_ENABLED=true
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL=30s